{
  "name": "@tdd-buffet/selenium",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.1",
  "description": "Spin up a Selenium grid with Chrome and Firefox nodes + VNC debugging",
  "keywords": [
    "selenium",
    "tests",
    "docker",
    "vnc"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NiGhTTraX/tdd-buffet.git"
  },
  "author": "Andrei Picus",
  "license": "MIT",
  "homepage": "https://github.com/NiGhTTraX/tdd-buffet/tree/master/packages/selenium",
  "bin": "./index.js",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist",
    "package": "cp package.json ./dist",
    "compile": "mkdir dist && cp -r src/* ./dist",
    "prepublishOnly": "npm run build && npm run package"
  },
  "dependencies": {
    "execa": "~2.0.3",
    "meow": "~5.0.0"
  }

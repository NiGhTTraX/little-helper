{
  "name": "tdd-buffet",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "version": "0.11.0",
  "description": "All you can eat TDD tools and libraries",
  "keywords": [
    "tdd",
    "helper",
    "testing"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NiGhTTraX/tdd-buffet.git"
  },
  "author": "Andrei Picus",
  "license": "MIT",
  "homepage": "https://github.com/NiGhTTraX/tdd-buffet/tree/master/packages/tdd-buffet",
  "bin": "./bin/index.js",
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "rm -rf ./dist",
    "package": "cp -r README.md package.json src/config ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepublishOnly": "yarn run build && yarn run package",
    "webpack": "COVERAGE=1 webpack --config ./src/config/webpack.config.js ./tests/coverage/webpack.ts -o ./tests/coverage/webpack-bundle.js"
  },
  "dependencies": {
    "@babel/core": "~7.6.0",
    "@jest/types": "~24.9.0",
    "@types/chai": "~4.2.0",
    "@types/fs-extra": "~8.0.0",
    "@types/istanbul-lib-coverage": "~2.0.1",
    "@types/jest": "~24.0.18",
    "babel-loader": "~8.0.6",
    "babel-plugin-istanbul": "~5.2.0",
    "chai": "~4.2.0",
    "css-loader": "~3.2.0",
    "execa": "~2.0.3",
    "fork-ts-checker-webpack-plugin": "~1.5.0",
    "fs-extra": "~8.1.0",
    "istanbul-lib-coverage": "~2.0.5",
    "jest": "~24.9.0",
    "jest-runtime": "~24.9.0",
    "less": "~3.10.3",
    "less-loader": "~5.0.0",
    "meow": "~5.0.0",
    "mini-css-extract-plugin": "~0.8.0",
    "postcss-flexbugs-fixes": "~4.1.0",
    "postcss-loader": "~3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "~6.7.0",
    "style-loader": "~1.0.0",
    "ts-jest": "~24.1.0",
    "ts-loader": "~6.1.2",
    "webdriverio": "~5.13.0",
    "webpack": "~4.41.0"
  },
  "devDependencies": {
    "@types/meow": "~5.0.0",
    "@types/node": "~12.7.0",
    "strong-mock": "~3.0.0",
    "typescript": "~3.6.0",
    "webpack-cli": "~3.3.9"
  }
}

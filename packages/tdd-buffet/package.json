{
  "name": "tdd-buffet",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "version": "0.1.0",
  "description": "All you can eat delicious TDD helpers, healthy configs and TypeScript beverages",
  "keywords": [
    "tdd",
    "helper",
    "testing"
  ],
  "repository": {
    "type": "git",

{
  "name": "tdd-buffet",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "version": "0.7.1",
  "description": "All you can eat TDD tools and libraries",
  "keywords": [
    "tdd",
    "helper",
    "testing"
  ],
